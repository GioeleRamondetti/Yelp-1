<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polito.tdp.Yelp.FXMLController">
   <center>
      <VBox prefHeight="200.0" prefWidth="274.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#handleLoadData" text="carica dati" />
            <TableView fx:id="tableTblBusiness">
              <columns>
                <TableColumn fx:id="colName" editable="false" prefWidth="75.0" text="name" />
                <TableColumn fx:id="colStars" editable="false" prefWidth="75.0" text="stars" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </center>
</BorderPane>
